{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Death Rates in Malaysia during 2022 by States (in 1000s)",
    "width": 800,
    "height": 400,
    "projection": {"type": "equalEarth"},
    "data": {
    "url": "https://raw.githubusercontent.com/qchu0005/FIT3179-W9/refs/heads/main/ne_10m_admin_1_states_provinces.json",
    "format": {"type": "topojson", "feature": "ne_10m_admin_1_states_provinces"}
    },
    "transform": [
    {
    "lookup": "properties.name",
    "from": {
    "data": {
    "url": "https://raw.githubusercontent.com/qchu0005/FIT3179-W9/refs/heads/main/statistic_id1040803_number-of-deaths-in-malaysia-2022-by-state.csv"
    },
    "key": "State",
    "fields": ["Rate"]
    }
    },
    {"calculate": "datum.Rate", "as": "Death Rates"}
    ],
    "mark": {"type": "geoshape"},
    "encoding": {
    "color": {
    "field": "Death Rates",
    "type": "quantitative",
    "scale": {"type": "log"}
    },
    "tooltip": [
    {"field": "properties.name", "type": "nominal", "title":
    "States"},
    {"field": "Death Rates", "type": "quantitative"}
    ]
    }
    }
